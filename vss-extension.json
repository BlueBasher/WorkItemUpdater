{
    "manifestVersion": 1,
    "id": "bluebasher-workitemupdater",
    "version": "2.3.766",
    "name": "WorkItem Updater",
    "scopes": [
        "vso.work",
        "vso.work_write"
    ],
    "description": "Update the state of work items linked to a build.",
    "publisher": "bluebasher",
    "public": false,
    "icons": {
        "default": "marketplace/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Boards",
        "Azure Pipelines"
    ],
    "tags": [
        "WorkItem",
        "DevOps",
        "State",
        "Fields",
        "Relation",
        "Build",
        "Build task",
        "Release",
        "Release task"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/BlueBasher/WorkItemUpdater/blob/master/src/overview.md"
        },
        "issues": {
            "uri": "https://github.com/BlueBasher/WorkItemUpdater/issues"
        },
        "repository": {
            "uri": "https://github.com/BlueBasher/WorkItemUpdater"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/bluebasher/WorkItemUpdater"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "branding": {
        "color": "rgb(141, 141, 141)",
        "theme": "light"
    },
    "files": [
        {
            "path": "marketplace",
            "addressable": true
        },
        {
            "path": "dist"
        }
    ],
    "contributions": [
        {
            "id": "bluebasher-workitemupdater",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "dist"
            }
        }
    ]
}